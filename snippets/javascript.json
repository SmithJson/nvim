{
    "const": {
      "prefix": "cv",
      "body": [
        "const $0 = $1;"
      ],
      "description": "const"
    },
    "let": {
      "prefix": "lv",
      "body": [
        "let $0 = $1;"
      ],
      "description": "let"
    },

    "import(别名导入)": {
      "prefix": "impa",
      "body": [
        "import * as $0 from '$1';"
      ],
      "description": "import(别名导入)"
    },
    "import(具名导入)": {
      "prefix": "impv",
      "body": [
        "import {$0} from '$1';"
      ],
      "description": "import(具名导入)"
    },
    "import(默认导入)": {
      "prefix": "imp",
      "body": [
        "import $0 from '$1';"
      ],
      "description": "import(默认导入)"
    },
    "import(默认导出)": {
      "prefix": "exp",
      "body": [
        "export default $0"
      ],
      "description": "import(默认导出)"
    },
    "import(具名导出)": {
      "prefix": "expv",
      "body": [
        "export {",
        "  $0",
        "};"
      ],
      "description": "import(具名导出)"
    },

    "consoleerror": {
      "prefix": "cle",
      "body": [
        "console.error($0);"
